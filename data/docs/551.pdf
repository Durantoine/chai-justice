(({ imageSelectors = ".image img" }) => {
	document.readyState !== "loading" ? true : new Promise(resolve => document.addEventListener("DOMContentLoaded", resolve));

	try {
		if(window.self !== window.top && window.top.location.hostname === "dpe.pwc.com") {
			return false; /* This script is running in an iFrame under dpe.pwc.com */
		}
	}
	catch (error) {
		console.error("Unable to detect environment", error);
	}

	const videoTemplate = ({ poster, videoPath }) => {
		return document.createRange().createContextualFragment(`
			<style>
				.embeddedvid {
					width: 100%;
					height: auto;
					aspect-ratio: 1 / 1;
					object-fit: cover;
				}
			</style>
			<video class="embeddedvid" autoplay muted loop poster="${ poster }" playsinline>
				<source src="${ videoPath }" type="video/mp4">
			</video>
		`);
	};

	document.querySelectorAll(imageSelectors).forEach((imgToVideo) => {
		const poster = imgToVideo.src;
		const videoPath = imgToVideo.title;

		imgToVideo.replaceWith(videoTemplate({ poster, videoPath }));
	});
})({
	imageSelectors: `.syc .image img[title *= ".mp4"]`
});
</script>
